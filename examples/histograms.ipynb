   "source": [
    "function getLowerBound(queries::HistogramQueries, eps, delta)\n",
    "    m,n = size(queries.queries)\n",
    "    P_eps_gamma = (2*log2(2/delta))/(eps^2)\n",
    "    singular_values = svdfact(queries.queries)[:S]\n",
    "    ss_svd = (1/n)*(sum(singular_values)^2)\n",
    "\n",
    "    ss_svd*P_eps_gamma\n",
    "end"
   ]
